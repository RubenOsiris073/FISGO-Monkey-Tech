  "dependencies": {
    "@stripe/react-stripe-js": "^3.7.0",
    "@stripe/stripe-js": "^7.3.1",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@zxing/library": "^0.20.0",
    "ajv": "^8.17.1",
    "axios": "^1.3.4",
    "bootstrap": "^5.2.3",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "firebase": "^9.23.0",
    "framer-motion": "^12.15.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.8.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "react-webcam": "^7.2.0",
    "react-zxing": "^1.1.4",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false FAST_REFRESH=true BROWSER=none react-scripts start",
    "start:fast": "GENERATE_SOURCEMAP=false FAST_REFRESH=true BROWSER=none SKIP_PREFLIGHT_CHECK=true react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "test": "react-scripts test",
    "test:lint": "eslint src/ --ext .js,.jsx --max-warnings 0",
    "lint": "eslint src/ --ext .js,.jsx",
    "lint:fix": "eslint src/ --ext .js,.jsx --fix",
    "eject": "react-scripts eject",
    "clean": "rm -rf node_modules/.cache && rm -rf build",
    "prestart": "npm run clean",
    "dev": "npm run start:fast"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-plugin-jest": "^28.11.0",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
